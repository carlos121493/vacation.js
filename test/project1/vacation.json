{
	"build":{
		"dest":"./app/min",
		"src":"./app/src",
		"base":"./app/src",
		"pkg":[
			{
				"main":["v_detail\\.js$"],
				"dest_rule":"$1.js",
				"sub":[
					{
						"dest_rule":"$pkg.js",
						"contain":[
							["\\bmod_[a-zA-Z_]+\\.js"]
						]
					}
				],
				"except":[
					["\\bjquery"],
					["\\bunderscore"]
				]
			}
		],
		"paths":{
			"lib":"./lib"
		},
		"alias":{
			"jquery":"lib/jquery",
			"underscore":"lib/underscore"
		},
		"available":[],
		"ignore":[
			["^vacation.json"],
			["^map.json"],
			["\\.md$"],
			["^lib/(inherit\\.min|jquery-.*|vacation.*)"],
			["^app/min/"],
			["^app/transport/"],
			["^app/src/(car|coupon|index|order|outie|private|promotions|search|study|tickets|visa)"]
		],
		"availableType":["js","css","html"],
		"uglify":{
			"banner":"/*! lastmodify: $$today('yyyy-mm-dd HH:MM:ss') */\n",
			"mangle":{
				"except":["$","require","exports","module"]
			}
		}
	},
	"server":{
		"port":8181,
		"defaultFile":"index.html",
		"root":"./",
		"rootRelative":"cwd",
		"allowOrigin":""
	},
	"contentType":{
		"json":"application/json"
	}
}
