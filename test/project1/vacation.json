{
	"build":{
		"dest":"./app/min",
		"src":"./app/src",
		"base":"./app/src",
		"pkg":[
			{
				"name":["v_detail\\.js$"],
				"rule":"$1.js"
			}
		],
		"paths":{
			"lib":"./lib"
		},
		"alias":{
			"jquery":"lib/jquery",
			"underscore":"lib/underscore"
		},
		"available":[],
		"ignore":[
			["^vacation.json"],
			["^map.json"],
			["\\.md$"],
			["^lib/(inherit\\.min|jquery-.*|vacation.*)"],
			["^app/min/"],
			["^app/transport/"],
			["^app/src/(car|coupon|index|order|outie|private|promotions|search|study|tickets|visa)"]
		],
		"availableType":["js","css","html"]
	},
	"server":{
		"port":8181,
		"defaultFile":"index.html",
		"root":"./",
		"rootRelative":"cwd",
		"allowOrigin":""
	},
	"contentType":{
		"json":"application/json"
	}
}
