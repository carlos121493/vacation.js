{
	"build":{
		"dest":"./dest",
		"src":"./src",
		"main":["main.js"],
		"base":"./src",
		"availableType":["js","css","html"]
	},
	"file":{
		"transport": true,
		"transportText": true,
		"concat": true,
		"optimize": true,
		"compileHandlebars": true
	},
	"server":{
		"port":8181,
		"defaultFile":"index.html",
		"root":"./",
		"rootRelative":"cwd",
		"allowOrigin":""
	},
	"contentType":{
		"json":"application/json"
	}
}
