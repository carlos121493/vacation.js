{
	"build":{
		"dest":"./dest",
		"src":"./src",
		"base":"./src",
		"www":"./",
		"pkg":[
			{
				"name":["main\\.js$"],
				"rule":"$1_$2_combo.js"
			}
		],
		"paths":{},
		"alias":{},
		"available":[],
		"ignore":[
			["^vacation.json"],
			["^map.json"],
			["\\.md$"],
			["(^|\\/)\\."]
		],
		"availableType":["js","css","html"],
		"uglify":{
			"banner":"/*! lastmodify: $$today('yyyy-mm-dd HH:MM:ss') */\\n",
			"mangle":{
				"except":["$","require","exports","module"]
			}
		}
	},
	"server":{
		"port":8181,
		"defaultFile":"index.html",
		"root":"./",
		"rootRelative":"cwd",
		"allowOrigin":""
	},
	"contentType":{
		"json":"application/json"
	}
}
