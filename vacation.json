{
	"build":{
		"dest":"./dest",
		"src":"./src",
		"base":"./src",
		"www":"./",
		"pkg":[
			{
				"main":["main\\.js$"],
				"dest_rule":"$1_$pkg_combo.js",
				"sub":[
					{
						"dest_rule":"$1_$2_$pkg.js",
						"contain":[
							["module_a\\.js"],
							["module_b\\.js"]
						]
					}
				],
				"except":[
					["\\bjquery"],
					["\\bunderscore"]
				]
			}
		],
		"paths":{},
		"alias":{},
		"available":[],
		"ignore":[
			["^vacation.json"],
			["^map.json"],
			["\\.md$"],
			["(^|\\/)\\."]
		],
		"availableType":["js","css","html"],
		"uglify":{
			"banner":"/*! lastmodify: $$today('yyyy-mm-dd HH:MM:ss') */\\n",
			"mangle":{
				"except":["$","require","exports","module"]
			}
		}
	},
	"server":{
		"port":8181,
		"defaultFile":"index.html",
		"root":"./",
		"rootRelative":"cwd",
		"allowOrigin":""
	},
	"contentType":{
		"json":"application/json"
	}
}
