{
	"build":{
		"dest":"./dest",
		"src":"./src",
		"main":{
			"main.js":"$1_combo.js"
		},
		"base":"./src",
		"availableType":["js","css","html"],
		"checkCMD": true,
		"notCMD":["handlebars"],
		"transport": true,
		"transportTpl": true,
		"optimize": true,
		"optimizeTpl":true,
		"concat": true,
		"compileHandlebars": true,
		"style":"inline"
	},
	"server":{
		"port":8181,
		"defaultFile":"index.html",
		"root":"./",
		"rootRelative":"cwd",
		"allowOrigin":""
	},
	"contentType":{
		"json":"application/json"
	}
}
